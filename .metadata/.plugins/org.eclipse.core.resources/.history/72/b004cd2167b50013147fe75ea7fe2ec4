<?php
$ch=curl_init("https://store-uzahrx.mybigcommerce.com/api/v2");
echo $ch."<br>";
//echo curl_setopt($ch, CURLOPT_URL,"https://store-uzahrx.mybigcommerce.com/api/v2")."<br>";
// grab URL and pass it to the browser
curl_exec($ch);

// close cURL resource, and free up system resources
curl_close($ch);


?>